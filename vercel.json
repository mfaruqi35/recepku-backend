{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api-docs.json",
      "dest": "/server.js"
    },
    {
      "src": "/swagger-ui-bundle\\.(js|css)",
      "dest": "/server.js"
    },
    {
      "src": "/swagger-ui\\.(js|css)",
      "dest": "/server.js"
    },
    {
      "src": "/swagger-ui-standalone-preset\\.js",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
